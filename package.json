{
  "author": "Octopus Deploy",
  "bugs": {
    "url": "https://github.com/OctopusDeploy/run-runbook-action/issues"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/http-client": "^1.0.11",
    "@actions/tool-cache": "^1.7.1"
  },
  "description": "GitHub Action to Run a Runbook in Octopus Deploy",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/parser": "^5.12.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.9.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "tmp": "^0.2.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "homepage": "https://github.com/OctopusDeploy/run-runbook-action#readme",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "setupFiles": [
      "<rootDir>/src/test-setup.ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "testRunner": "jest-circus/runner",
    "testTimeout": 10000,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true
  },
  "keywords": [
    "cli",
    "deployment",
    "octopus-deploy",
    "github-actions"
  ],
  "license": "Apache-2.0",
  "main": "out/index.js",
  "name": "run-runbook-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OctopusDeploy/run-runbook-action.git"
  },
  "scripts": {
    "all": "npm run format && npm run build && npm test",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "prebuild": "eslint src/**/*.ts",
    "postbuild": "ncc build -o dist",
    "test": "jest"
  },
  "version": "1.0.1"
}
